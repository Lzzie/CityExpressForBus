<?xml version="1.0" encoding="utf-8"?>
<!-- android:shape指定形状类型，默认为rectangle -->
<!-- 在描边里面设置了dash参数，使得图形的边变成了虚线 <stroke   android:dashGap="3dp"  android:dashWidth="5dp"  android:color="#dcdcdc" />  -->
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    >
    <solid android:color="@color/register_no" />

    <stroke android:width="0dip" android:color="#ededed" />

    <corners android:radius="5dp" /> <!-- 设置四个角的半径 -->



</shape>